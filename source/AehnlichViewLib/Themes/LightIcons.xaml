    <BitmapImage  x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type reskeys:ResourceKeys},
                  ResourceId=ICON_GotoNextKey}"
                  UriSource="/AehnlichViewLib;component/Icons/Light/GotoNext.ico" />

    <BitmapImage  x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type reskeys:ResourceKeys},
                  ResourceId=ICON_GotoPrevKey}"
                  UriSource="/AehnlichViewLib;component/Icons/Light/GotoPrev.ico" />

    <BitmapImage  x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type reskeys:ResourceKeys},
                  ResourceId=ICON_GotoBottomKey}"
                  UriSource="/AehnlichViewLib;component/Icons/Light/GotoBottom.ico" />
    <!-- Goto Icons-->

</ResourceDictionary>
